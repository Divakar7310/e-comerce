{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/admin/.*",
        "dest": "/admin/index.html"
      },
      {
        "src": "/api/.*",
        "dest": "/backend/index.js"
      },
      {
        "src": "/.*",
        "dest": "/frontend/src/index.html"
      }
    ]
  }
  